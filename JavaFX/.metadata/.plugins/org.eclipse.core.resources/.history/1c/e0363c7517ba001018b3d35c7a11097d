package application;

import javafx.event.ActionEvent;
import javafx.fxml.FXML;

public class MainController {

	@FXML
	void login(ActionEvent event) {
		try {
			Main.setRoot("Login");
		} catch (Exception e) {
			System.err.println("Login.fxml 파일이 없습니다.");
			e.printStackTrace();
		}
	}
	
	@FXML
	void join(ActionEvent event) {
		try {
			Main.setRoot("Join");
		} catch (Exception e) {
			System.err.println("Join.fxml 파일이 없습니다.");
			e.printStackTrace();
		}
	}
	
	@FXML
	void menu(ActionEvent event) {
		try {
			Main.setRoot("Menu");
		} catch (Exception e) {
			System.err.println("Menu.fxml 파일이 없습니다.");
			e.printStackTrace();
		}
	}
	
    @FXML
    void board(ActionEvent event) {
    	try {
			Main.setRoot("Board");
		} catch (Exception e) {
			System.err.println("Board.fxml 파일이 없습니다.");
			e.printStackTrace();
		}
    }
}
